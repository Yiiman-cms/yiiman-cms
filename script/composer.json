{
  "name": "yiiman/cms",
  "description": "Yii 2 Advanced Project Template with basic cms features",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "project template",
    "yii2 cms",
    "yii2 template"
  ],
  "homepage": "http://yiiman.ir/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiiman-cms/yiiman-cms/issues?state=open",
    "forum": "http://forum.yiiman.ir",
    "wiki": "http://docs.yiiman.ir",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiiman-cms/yiiman-cms"
  },
  "minimum-stability": "dev",
  "require": {
    "yiiman/yii-basics": "dev-main",
    "yidas/yii2-bower-asset": "~2.0.5",
    "yiimaker/yii2-google-analytics": "*",
    "execut/yii2-robots-txt": "dev-master",
    "symfony/browser-kit": ">=2.7 <=4.2.4",
    "farsidesign/yii2-sbadminrtl": "dev-master",
    "guzzlehttp/guzzle": "^6.0",
    "kriswallsmith/buzz": "^0.16",
    "consik/yii2-websocket": "^1.0",
    "ezyang/htmlpurifier": "*",
    "mohammad-mahdy/yii2-jdate": "*",
    "faravaghi/yii2-jalali-daterangepicker": "*",
    "ogheo/yii2-htmlcompress": "*",
    "toin0u/digitalocean-v2": "^2.2",
    "kartik-v/yii2-money": "*",
    "kartik-v/yii2-field-range": "*",
    "kartik-v/yii2-widget-datepicker": "@dev",
    "mrclay/minify": "2.1.7",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-gd": "*",
    "yiiman/yii-module-setting": "dev-main",
    "yiiman/functions": "dev-main",
    "yiiman/yii-lib-cookie": "dev-main",
    "insolita/yii2-migration-generator": "dev-master",
    "yiiman/yii-module-user": "dev-main"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "~2.0.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "platform": {
      "php": "7.2.14",
      "ext-fileinfo": "1.0.5",
      "ext-pdo": "7.2.14",
      "ext-session": "7.2.14",
      "ext-iconv": "7.2.14",
      "ext-zip": "1.15.4",
      "ext-dom": "1.8.2",
      "ext-gd": "1.8.2",
      "ext-libxml": "1.8.2"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php install"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]

}
